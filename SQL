CREATE TABLE OPENING (
OPENING_NUMBER varchar(8) PRIMARY KEY,
COMPANY_NAME varchar(30),
QUALIFICATION_CODE varchar(50),
START_DATE datetime, 
END_DATE datetime,	
HOURLY_PAY DECIMAL(5,2) 
);

