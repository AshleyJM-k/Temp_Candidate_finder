CREATE TABLE CANDIDATE (
CANDIDATE_ID int PRIMARY KEY,
CANDIDATE_NAME varchar(20),
CANDIDATE_PHONE varchar(12),
CANDIDATE_ADDRESS varchar(20),
CANDIDATE_EMAIL varchar(20)
);
