CREATE TABLE COMPANY (
COMPANY_NAME varchar(30),
COMPANY_ADDRESS varchar(100),
COMPANY_PHONE varchar(12),
COMPANY_EMAIL varchar(40)
); 

INSERT INTO COMPANY VALUES 

/* -- */ 

CREATE TABLE OPENING (
OPENING_NUMBER varchar(8)
COMPANY_NAME varchar(30),
QUALIFICATION_CODE char,
START_DATE datetime,    
END_DATE datetime,         
HOURLY_PAY float(5,2)    
); 

INSERT INTO OPENING VALUES
/* -- */ 

CREATE TABLE QUALIFICATION (
QUALIFICATION_CODE char
CANDIDATE_ID char
DESCRIPTION varchar(40)
); 

INSERT INTO QUALIFICATION VALUES
/* -- */ 

CREATE TABLE PLACEMENT (
OPENING_NUMBER int, 
COMPANY_NAME varchar(30),
HOURS float (3,0)
); 

INSERT INTO PLACEMENT VALUES
/* -- */



CREATE TABLE CANDIDATE (
CANDIDATE_ID number,
CANDIDATE_NAME varchar(20),
CANDIDATE_PHONE varchar(12),
CANDIDATE_ADDRESS varchar(20),
CANDIDATE_EMAIL varchar(20)
); 

INSERT INTO CANDIDATE VALUES 

/* -- */ 

CREATE TABLE HISTORY (
HISTORY_CODE
COMPANY_NAME
); 

INSERT INTO HISTORY VALUES 

/* -- */ 

CREATE TABLE SESSION (
SESSION_ID
COURSE_CODE
START_TIME
END_TIME
SESSION_FEE
CANDIDATE_ID
); 

INSERT INTO SESSION VALUES
/* -- */ 

CREATE TABLE COURSE
COURSE_CODE
COURSE_NAME
COURSE_DESCRIPTION
);
